datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
}

model Contact {
  id              Int      @id @default(autoincrement())
  first_name      String
  last_name       String
  email           String   @unique
  service         String
  phone           String?
  details         String?
  createdAt       DateTime @default(now())
}
